<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    tools:context=".MainActivity">



    <com.mt.component_util.ui.view.ScaleSeekBar
        app:text_color="@color/teal_200"
        app:start_progress="0"
        app:end_progress="100"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.mt.component_util.ui.view.ScaleSeekBar>


    <com.mt.component_util.ui.view.SwitchButton
        app:bg_color="@color/cardview_dark_background"
        app:ball_color="@color/white"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

    </com.mt.component_util.ui.view.SwitchButton>

</LinearLayout>